{
	"manifest_version": 2,

	"name": "Marker scrolling",
	"version": "0.1",

	"icons": {
		"128": "img/128.png",
		"48" : "img/48.png",
		"16" : "img/16.png"
	},
	"browser_action": {
		"default_title": "Marker scrolling",
		"default_icon": "img/48.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [ {
      "js": [ "js/jquery-2.1.3.min.js", "js/marker.js"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ]

}